    "build": "npm run build:lib",
    "build:lib": "ng build ngx-imageviewer --prod && cp README.md node_modules/ngx-imageviewer",
    "build:docs": "ng build ngx-imageviewer-app --prod",
    "postbuild": "tar -C dist -czf dist/ngx-imageviewer.tgz ngx-imageviewer",
